<Window x:Class="rename.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rename"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="39,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="288"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="39,52,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="288"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="4,25,0,0" TextWrapping="Wrap" Text="Path" VerticalAlignment="Top" Height="23"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="4,53,0,0" TextWrapping="Wrap" Text="Prefix" VerticalAlignment="Top" Height="23"/>
        <Button x:Name="button" Content="Done" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>
</Window>
